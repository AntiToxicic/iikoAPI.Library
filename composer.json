{
    "name": "iikoapilibrary/iikoapi",
    "type": "library",
    "description": "The library provides method for work with iikoAPI",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
        "illuminate/support": "^8.0 || ^9.0 || ^10.0",
        "illuminate/http": "^8.0 || ^9.0 || ^10.0",
        "illuminate/cache": "^8.0 || ^9.0 || ^10.0",
        "symfony/http-kernel": "^5.3 || ^6.0"
    },
    "require-dev": {
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "IikoApiLibrary\\Providers\\iikoServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "IikoApiLibrary\\": "src/"
        },
        "classmap": [

        ]
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {

    }
}
